{
  "version": 2,
  "builds": [
    {
      "src": "routes/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.+\\.html)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.css)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/routes$1"
    }
  ]
}
